<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">ID</th>
      <th scope="col">Full name</th>
      <th scope="col">Email</th>
      <th scope="col">Gender</th>
      <th scope="col">Phone</th>
    </tr>
  </thead>
  <tbody>
    {{#each newUsers}}
      <tr>
        <th scope="row">{{row}}</th>
        <td>{{id}}</td>
        <td>{{fullName}}</td>
        <td>{{email}}</td>
        <td>{{gender}}</td>
        <td>{{phone}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>